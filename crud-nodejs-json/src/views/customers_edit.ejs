          <div class="form-group">
            <input type="text" name="address" value="<%= data.address%>" placeholder="TAMAÑO" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="phone" value="<%= data.phone%>" placeholder="PRECIO" class="form-control">
          </div>
          
         

          <button type="submit" class="btn btn-primary">
            Guardar
          </button>
        </form>
      </div>
    </div>
  </div>
</div>
